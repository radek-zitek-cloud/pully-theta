# Development environment variables for Vue.js application
# These values override .env during development

# Application Configuration
VITE_APP_TITLE="Pully Theta (Dev)"
VITE_APP_VERSION="1.0.0-dev"

# API Configuration
# Use localhost for development
VITE_API_BASE_URL="http://localhost:6910/api/v1"

# Feature Flags
VITE_ENABLE_MOCK_API="false"

# Authentication Configuration
VITE_JWT_SECRET="dev-jwt-secret-key"

# Development/Debug Settings
VITE_ENABLE_DEVTOOLS="true"
VITE_LOG_LEVEL="debug"
